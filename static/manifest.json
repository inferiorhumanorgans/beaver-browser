{
  "key":              "",
  "manifest_version": 2,
  "name":             "beaverbrowser",
  "description":      "Beaver Chrome extension for zx2c4's pass (password manager).",
  "version":          "0.0.1",
  "author":           "Alex Zepeda",
  "homepage_url":     "https://github.com/inferiorhumanorgans/beaver-browser",
  "browser_action": {
    "default_icon":   "beaver.png",
    "default_popup":  "index.html"
  },
  "permissions": [
    "activeTab",
    "nativeMessaging",
    "http://*/*",
    "https://*/*"
  ],
  "commands": {
   "_execute_browser_action": {
     "suggested_key": {
       "default":     "Alt+Shift+Comma"
     }
   }
 }
}
